{
  "kind": "collectionType",
  "collectionName": "portfoliocontacts",
  "info": {
    "singularName": "contact",
    "pluralName": "portfoliocontacts",
    "displayName": "Contact"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Email": {
      "type": "email",
      "required": true
    },
    "phone": {
      "type": "string",
      "minLength": 9,
      "required": true
    },
    "location": {
      "type": "richtext",
      "required": true,
      "default": "Ahmedabad, India"
    },
    "name": {
      "type": "string",
      "default": "Yash Patel",
      "required": true
    },
    "githubURL": {
      "type": "string",
      "required": true
    },
    "linkdinURL": {
      "type": "string",
      "required": true
    },
    "profileIMG": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "userRole": {
      "type": "string"
    }
  }
}
