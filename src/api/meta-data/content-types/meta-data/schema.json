{
  "kind": "collectionType",
  "collectionName": "portfolio_meta_data",
  "info": {
    "singularName": "meta-data",
    "pluralName": "portfolio-meta-data",
    "displayName": "Meta Data"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "metatitle": {
      "type": "string",
      "default": "Yash Patel | Portfolio",
      "required": true
    },
    "metaDescription": {
      "type": "richtext",
      "required": true
    },
    "shareImage": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "metakeywords": {
      "type": "component",
      "component": "seo.seo",
      "repeatable": true
    }
  }
}
